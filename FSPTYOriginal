/////////////////////////Help Windows////////////////////////////////////////////////////////////

    		var helpWindow = new Ext.Window({
        		layout: "fit",
        		hideBorders: true,
        		closeAction: "hide",
        		width: 300,
        		height: "auto",
        		title: "Help",
        			items: {contentEl: "help",
            				collapsible: false,
            				split: true}
 				});

    		function helpWindowOpen() {
									   helpButton.deactivate();
									   helpWindow.show();
		    					}

    		var importHelpWindow = new Ext.Window({
        		layout: "fit",
        		hideBorders: true,
        		closeAction: "hide",
        		width: 300,
        		height: "auto",
        		title: "Import Coordinates Help",
        			items: {contentEl: "importHelp",
            				collapsible: false,
            				split: true}
 				});

    		function importHelpWindowOpen() {
    								   importHelpWindow.show();
		    					}

